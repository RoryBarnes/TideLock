# Time for moonless Earth to lock
sSystemName           earth
sTideModel            CPL
bDiscreteRot          1
#sTideModel            CTL

iVerbose              5
iDigits               8
iSciNot               4

bDoLog                1

sUnitMass             solar
sUnitLength           AU
sUnitTime             year
sUnitAngle            degrees

bDoForward            1
bVarDt                1
dForwardStopTime      1.5e10
dForwardOutputTime    1e6
sForwardFile          earth.forward
dTimestepCoeff        0.01
dMinValue             1e-10
bHaltSecLock	      1

dPrimaryMass          1
dPrimaryRadius        -1
dPrimarySpinPeriod    -30     # days
dPrimaryObliquity     0
dPrimaryRadGyra       0.5
dPrimaryK2            0.5
dPrimaryQ             1e6
dPrimaryTau             -0.01

dSecondaryMass        -1
dSecondaryRadius      -1
dSecondarySpinPeriod  -3
dSecondaryObliquity   0
dSecondaryK2          0.3
dSecondaryRadGyra     0.5
dSecondaryQ           12
dSecondaryTau         -640
dSecondaryMaxLockDiff 0.01

dSemi                 1
dEcc                  0

sOutputOrder          Time Semi Ecc -SecPer SecObl -SecEqSurfFlux
